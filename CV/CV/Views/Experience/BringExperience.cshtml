@model CV.Models.Entity.Experience
@{
    ViewBag.Title = "BringExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<header class="d-flex justify-content-center py-3 bg-white border-bottom">
    <h2>Bring Experience Page</h2>
</header>
<br />
<form method="post">
    @Html.Label("Experience ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.ID, "", new { @style = "color:red" })
    <br />
    @Html.Label("Company")
    @Html.TextBoxFor(x => x.COMPANY, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.COMPANY, "", new { @style = "color:red" })
    <br />
    @Html.Label("Position")
    @Html.TextBoxFor(x => x.POSITION, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.POSITION, "", new { @style = "color:red" })
    <br />
    @Html.Label("Sector")
    @Html.TextBoxFor(x => x.SECTOR, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.SECTOR, "", new { @style = "color:red" })
    <br />
    @Html.Label("Enter Way of Working")
    @Html.TextBoxFor(x => x.WAYOFWORKING, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.WAYOFWORKING, "", new { @style = "color:red" })

    <br />
    @Html.Label("Start")
    <input type="date" name="START" class="form-control" />
    @Html.ValidationMessageFor(x => x.START, "", new { @style = "color:red" })
    <div class="text-black-50">
        Choose Start Date
    </div>
    <br />
    @Html.Label("Finish")
    <input type="date" name="FINISH" class="form-control" />
    @Html.ValidationMessageFor(x => x.FINISH, "", new { @style = "color:red" })
    <div class="text-black-50">

        Choose Finish Date
    </div>
    <br />
    <button class="btn btn-info">Update</button>
</form>