
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>CV Admin Panel</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/AdminLTE-3.0.4/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/Default/Index/" class="brand-link">
                <img src="/AdminLTE-3.0.4/dist/img/CVLogo.png"
                     alt="AdminLTE Logo"
                     class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">Go to CV</span>
            </a>

            <nav class="main-header navbar navbar-expand-md navbar-light navbar-white">

            </nav>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="/AdminLTE-3.0.4/dist/img/AdminLogo.png" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Admin Panel</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
    with font-awesome or any other icon font library -->
                        <li class="nav-item has-treeview">
                            <a href="/Welcome/Index" class="nav-link">
                                <i class="nav-icon fas fa-home"></i>
                                <p>
                                    Welcome
                                </p>
                            </a>

                        </li>
                        <li class="nav-item">
                            <a href="/About/Index/" class="nav-link">
                                <i class="nav-icon fas fa-file-word"></i>
                                <p>
                                    About
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Experience/Index/" class="nav-link">
                                <i class="nav-icon fas fa-business-time"></i>
                                <p>
                                    Experience
                                </p>
                            </a>

                        </li>
                        <li class="nav-item has-treeview">
                            <a href="/Education/Index/" class="nav-link">
                                <i class="nav-icon fas fa-school"></i>
                                <p>
                                    Education
                                </p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="/Skill/Index/" class="nav-link">
                                <i class="nav-icon far fa-star"></i>
                                <p>
                                    Skills
                                </p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="/Portfolio/Index/" class="nav-link">
                                <i class="nav-icon far fa-file"></i>
                                <p>
                                    Portfolio
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Interest/Index/" class="nav-link">
                                <i class="nav-icon far fa-image"></i>
                                <p>
                                    Interests
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Certificate/Index/" class="nav-link">
                                <i class="nav-icon far fa-id-card"></i>
                                <p>
                                    Certificates
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Language/Index/" class="nav-link">
                                <i class="nav-icon far fa-bell"></i>
                                <p>
                                    Languages
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/SocialMedia/Index/" class="nav-link">
                                <i class="nav-icon far fa-circle"></i>
                                <p>
                                    Social Media
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Contact/Index/" class="nav-link">
                                <i class="nav-icon far fa-envelope"></i>
                                <p>
                                    Contact
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Admin/Index/" class="nav-link">
                                <i class="nav-icon far fa-pen"></i>
                                <p>
                                    Settings
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Login/LogOut/" class="nav-link">
                                <i class="nav-icon far fa-user"></i>
                                <p>
                                    LogOut
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <div class="col-md-12">

                        @RenderBody()

                    </div>
                    </div>
            </section>
        </div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/AdminLTE-3.0.4/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/AdminLTE-3.0.4/dist/js/demo.js"></script>
</body>
</html>